#include<stdio.h>
int main()
{
    int a[3][3],i,j,sum;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("element - [%d],[%d] : ",i,j);
            scanf("%d",&a[i][j]);
        }
    }
    printf("\n\nThe matrix is :\n\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf(" %d ",a[i][j]);
            sum=sum+a[i][j];
        }
        printf("\n");
    }
    printf("\nthe sum of all the diagonal elements are : %d",sum);
    return 0;
}
